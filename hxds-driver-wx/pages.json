				"path": "wallet/wallet",
				"style": {
					"navigationBarTitleText": "我的钱包",
					"enablePullDownRefresh": false
				}

			}]
		},
		{
			"root": "execution",
			"pages": [{
				"path": "move/move",
				"style": {
					"navigationBarTitleText": "司乘同显",
					"enablePullDownRefresh": false
				}

			}]
		},
		{
			"root": "order",
			"pages": [{
				"path": "order/order",
				"style": {
