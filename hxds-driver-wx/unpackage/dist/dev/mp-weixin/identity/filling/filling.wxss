page {
  background-color: #eef0f6;
  height: 100%;
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding-bottom: 70rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-top: 120rpx;
}
.notice {
  background-color: #fee7a3;
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  width: 100%;
  padding: 20rpx 30rpx;
}
.notice text {
  font-size: 26rpx;
  color: #ac9146;
  margin-left: 10rpx;
}
.credentials-container {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.credentials-container .credentials {
  width: 214rpx;
  height: 150rpx;
  margin-bottom: 60rpx;
  position: relative;
}
.credentials-container .credentials .bg {
  width: 214rpx;
  height: 150rpx;
  display: block;
  border-radius: 10rpx;
}
.credentials-container .credentials .cover {
  position: absolute;
  left: 0;
  top: 0;
  width: 214rpx;
  height: 150rpx;
}
.credentials-container .credentials .cover .card {
  width: 46rpx;
  margin-top: 20rpx;
  margin-left: 25rpx;
}
.credentials-container .credentials .cover .desc {
  display: block;
  font-size: 24rpx;
  color: #fff;
  margin-left: 25rpx;
  margin-top: 0rpx;
  line-height: 1;
  font-weight: bold;
}
.credentials-container .credentials .cover .camera {
  border: none;
  background-color: #2074fd;
  width: 70rpx;
  height: 70rpx;
  border-radius: 35rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: 15rpx;
  background-image: url(data:image/png;base64,);
  background-size: 40rpx;
  background-repeat: no-repeat;
  background-position: 16rpx 15rpx;
}
.credentials-container .credentials .cover .camera:active {
  width: 70rpx;
  height: 70rpx;
  border-radius: 35rpx;
  background-color: #0259e9;
}
.title {
  font-weight: bold;
  font-size: 32rpx;
  color: #444;
  margin-top: 25rpx;
  margin-bottom: 25rpx;
  padding-left: 10rpx;
}
.list {
  border-radius: 20rpx;
  background-color: #fff;
  overflow: hidden;
}
.btn {
  background-color: #2074fd;
  color: #fff;
  margin-top: 40rpx;
  border: none;
  padding-top: 8rpx;
  padding-bottom: 8rpx;
}
.btn::after {
  border: none;
}
.btn:active {
  background-color: #0259e9;
}
.remark {
  color: #a0a0a0;
  font-size: 28rpx;
  margin-top: 30rpx;
  line-height: 1.7;
  bottom: 60rpx;
}

