page {
  background-color: #eef0f6;
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding-bottom: 0rpx;
}
.summary-container {
  background-color: #595e72;
  display: flex;
  padding: 40rpx 40rpx;
}
.summary-container .photo {
  width: 120rpx;
  border-radius: 60rpx;
}
.summary-container .summary {
  margin-left: 30rpx;
  flex-grow: 1;
}
.summary-container .summary .row {
  display: flex;
  padding-top: 10rpx;
}
.summary-container .summary .row .name {
  font-size: 40rpx;
  color: #e5e5e5;
  font-weight: bold;
}
.summary-container .summary .row .auth-icon {
  width: 36rpx;
  margin-top: 13rpx;
  margin-left: 30rpx;
}
.summary-container .summary .row .auth {
  color: #32b374;
  font-size: 28rpx;
  margin-top: 12rpx;
  margin-left: 15rpx;
}
.summary-container .summary .row .unauth {
  color: #32b374;
  font-size: 28rpx;
  margin-top: 12rpx;
  margin-left: 15rpx;
  color: #ffd900;
}
.summary-container .summary .row .attribute {
  color: #e5e5e5;
  font-size: 26rpx;
  width: 100%;
  display: flex;
  margin-top: 5rpx;
}
.summary-container .summary .row .attribute .item {
  line-height: 1;
}
.summary-container .summary .row .attribute .item:first-of-type {
  padding-right: 28rpx;
  border-right: solid 1rpx #e0e0e0;
}
.summary-container .summary .row .attribute .item:nth-of-type(2) {
  padding-left: 28rpx;
  padding-right: 28rpx;
  border-right: solid 1rpx #e0e0e0;
}
.summary-container .summary .row .attribute .item:nth-of-type(3) {
  padding-left: 28rpx;
}
.notice-container {
  background-color: #6f768e;
  padding: 30rpx 40rpx;
}
.notice-container .notice-title {
  display: flex;
}
.notice-container .notice-title image {
  width: 36rpx;
  margin-top: 8rpx;
}
.notice-container .notice-title text {
  color: #e5e5e5;
  font-size: 32rpx;
  margin-left: 10rpx;
  font-weight: bold;
}
.notice-container .desc {
  color: #e5e5e5;
  font-size: 26rpx;
  margin-top: 10rpx;
  line-height: 1.6;
}
.nav-container {
  background-color: #fff;
  padding: 0 40rpx;
}
.nav-container .row {
  display: flex;
  padding: 40rpx 0;
}
.nav-container .row .item {
  display: flex;
  width: 334rpx;
}
.nav-container .row .item:last-child {
  padding-left: 40rpx;
}
.nav-container .row .item image {
  width: 48rpx;
  margin-top: 4rpx;
}
.nav-container .row .item .info {
  margin-left: 20rpx;
}
.nav-container .row .item .info .title {
  display: block;
  font-size: 36rpx;
}
.nav-container .row .item .info .desc {
  display: block;
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #a0a0a0;
}
.nav-container .row .border-column {
  width: 0;
  border-left: solid 2rpx #f0f0f0;
}
.nav-container .border-row {
  display: flex;
  justify-content: space-between;
}
.nav-container .border-row::before {
  content: '';
  display: block;
  height: 0;
  border-bottom: solid 2rpx #f0f0f0;
  width: 300rpx;
}
.nav-container .border-row::after {
  content: '';
  display: block;
  height: 0;
  border-bottom: solid 2rpx #f0f0f0;
  width: 300rpx;
}
.tab-container {
  display: flex;
  margin: 20rpx 0;
  padding: 40rpx;
  background-color: #fff;
  justify-content: space-between;
}
.tab-container .item .ball {
  width: 90rpx;
  height: 90rpx;
  border-radius: 44rpx;
  background-color: #f3f3f3;
  background-size: 44rpx;
  background-repeat: no-repeat;
  margin-left: auto;
  margin-right: auto;
}
.tab-container .item .ball-1 {
  background-image: url(data:image/png;base64,);
  background-position: 26rpx 24rpx;
  background-size: 40rpx;
}
.tab-container .item .ball-2 {
  background-image: url(data:image/png;base64,);
  background-position: 24rpx 22rpx;
}
.tab-container .item .ball-3 {
  background-image: url(data:image/png;base64,);
  background-position: 24rpx 22rpx;
}
.tab-container .item .ball-4 {
  background-image: url(data:image/png;base64,);
  background-position: 24rpx 22rpx;
}
.tab-container .item text {
  font-size: 28rpx;
  color: #555;
  display: block;
  text-align: center;
  margin-top: 20rpx;
}
.exit {
  margin-top: 20rpx;
  background-color: #fff;
  padding-bottom: 50rpx;
}
.exit .btn {
  background-color: #2074fd;
  color: #fff;
  border-radius: 0;
  padding: 8rpx;
}
.exit .btn::after {
  border: none;
}
.exit .btn:active {
  background-color: #0259e9;
}

