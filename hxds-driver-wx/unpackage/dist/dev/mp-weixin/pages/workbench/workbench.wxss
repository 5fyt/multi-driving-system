page {
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.review-container image {
  display: block;
  width: 70%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 180rpx;
}
.review-container .title {
  color: #f54e4d;
  font-size: 50rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 50rpx;
}
.review-container .desc {
  font-size: 28rpx;
  color: #999;
  text-align: center;
  margin-top: 15rpx;
}
.top-container {
  background-color: #595e72;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  height: 200px;
}
.top-container .notice {
  color: #999faf;
  font-size: 26rpx;
  text-align: center;
  background-color: #4e5264;
  width: 60%;
  margin-left: auto;
  margin-right: auto;
  height: 48rpx;
  line-height: 48rpx;
  border-radius: 24rpx;
}
.top-container .summary-container {
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}
.top-container .summary-container .item {
  flex-grow: 1;
  color: #e5e5e5;
}
.top-container .summary-container .item:nth-child(2) {
  border-left: solid #e0e0e0 2rpx;
  border-right: solid #e0e0e0 2rpx;
}
.top-container .summary-container .item .title {
  text-align: center;
  font-size: 30rpx;
}
.top-container .summary-container .item .value {
  text-align: center;
  margin-top: 15rpx;
}
.top-container .summary-container .item .value .number {
  font-size: 56rpx;
  font-weight: bold;
  padding: 0 10rpx;
}
.top-container .summary-container .item .value .unit {
  font-size: 28rpx;
  vertical-align: 5rpx;
}
.top-container .service-container {
  display: flex;
  padding: 0 30rpx;
  justify-content: space-between;
  margin-top: 15rpx;
}
.top-container .service-container .item {
  margin-top: 20rpx;
  color: #46b68f;
  font-size: 24rpx;
  background-color: #4e5264;
  padding: 10rpx 15rpx;
  display: flex;
  border-radius: 10rpx;
}
.top-container .service-container .item .icon-1 {
  width: 30rpx;
  margin-right: 10rpx;
  margin-top: 3rpx;
}
.top-container .service-container .item .icon-2 {
  width: 30rpx;
  margin-right: 10rpx;
  margin-top: 3rpx;
}
.top-container .service-container .item .icon-3 {
  width: 33rpx;
  margin-right: 10rpx;
  margin-top: 1rpx;
}
.top-container .service-container .item .icon-4 {
  width: 30rpx;
  margin-right: 10rpx;
  margin-top: 3rpx;
}
.location {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 45rpx;
  bottom: 60rpx;
}
.new-order {
  border-top: solid 25rpx #e5e5e5;
  border-bottom: solid 25rpx #e5e5e5;
  padding-left: 50rpx;
  padding-right: 50rpx;
  padding-top: 50rpx;
  height: 570rpx;
}
.new-order .no-order-img {
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-top: 20rpx;
  width: 50%;
}
.new-order .no-order-title {
  font-size: 36rpx;
  text-align: center;
  margin-top: -30rpx;
  color: #fea802;
}
.new-order .line-1 {
  display: flex;
  justify-content: space-between;
}
.new-order .line-1 .sub-time {
  color: #b0b0b0;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  height: 40rpx;
}
.new-order .line-1 .number {
  color: #444;
  font-weight: bold;
  font-size: 60rpx;
  margin-right: 8rpx;
}
.new-order .line-1 .unit {
  color: #555;
  font-size: 32rpx;
}
.new-order .line-1 .tag {
  color: #f09054;
  font-size: 26rpx;
  margin-bottom: 10rpx;
  background-color: #fceade;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 20rpx;
  border-radius: 25rpx;
  position: relative;
  top: -10rpx;
}
.new-order .line-2 {
  font-size: 28rpx;
  color: #555;
  line-height: 1.6;
  margin-top: 35rpx;
}
.new-order .line-2 .from {
  display: flex;
}
.new-order .line-2 .from::before {
  display: block;
  content: '';
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background-color: #34ba97;
  margin-top: 14rpx;
  margin-right: 17rpx;
}
.new-order .line-2 .dashed-line {
  height: 30rpx;
  border-left: #ccc 2rpx dashed;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  margin-left: 10rpx;
}
.new-order .line-2 .to {
  display: flex;
}
.new-order .line-2 .to::before {
  display: block;
  content: '';
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background-color: #ff9d5b;
  margin-top: 14rpx;
  margin-right: 17rpx;
}
.new-order .accept-btn {
  margin-top: 50rpx;
  background-color: #ff5b5b;
  color: #fff;
  line-height: 70rpx;
  padding: 10rpx 0;
  font-size: 34rpx;
  font-weight: bold;
}
.new-order .accept-btn::after {
  border: none;
}
.new-order .accept-btn:active {
  background-color: #ff4141;
}
.new-order .disabled-btn {
  background-color: #aaaaaa;
  margin-top: 50rpx;
  color: #fff;
  line-height: 70rpx;
  padding: 10rpx 0;
  font-size: 34rpx;
  font-weight: bold;
}
.new-order .disabled-btn::after {
  border: none;
}
.notice-container {
  padding: 50rpx;
}
.notice-container .title {
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  margin-bottom: 15rpx;
}
.notice-container .desc {
  font-size: 28rpx;
  color: #a0a0a0;
  line-height: 1.7;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  display: flex;
}
.notice-container .desc .num {
  margin-right: 10rpx;
}
.execute-order-container {
  position: relative;
}
.execute-order-container .call {
  position: absolute;
  right: 45rpx;
  margin-top: 60rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  background-color: #44c762;
  background-image: url(data:image/png;base64,);
  background-size: 60rpx 60rpx;
  background-position: 26rpx 35rpx;
  background-repeat: no-repeat;
}
.execute-order-container .call:active {
  background-color: #38ba55;
}
.execute-order-container .customer-container {
  border-top: solid 25rpx #e5e5e5;
  display: flex;
  padding-left: 50rpx;
  padding-right: 50rpx;
  padding-top: 50rpx;
}
.execute-order-container .customer-container .info {
  margin-left: 30rpx;
}
.execute-order-container .customer-container .info .customer-name {
  font-size: 36rpx;
  font-weight: bold;
  color: #555;
  margin-bottom: 10rpx;
}
.execute-order-container .customer-container .info .customer-tel {
  font-size: 28rpx;
  color: #555;
  margin-bottom: 10rpx;
  letter-spacing: 1rpx;
}
.execute-order-container .address-container {
  font-size: 28rpx;
  color: #555;
  line-height: 1.6;
  margin-top: 35rpx;
  padding-left: 50rpx;
  padding-right: 50rpx;
}
.execute-order-container .address-container .from {
  display: flex;
}
.execute-order-container .address-container .from::before {
  display: block;
  content: '';
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background-color: #34ba97;
  margin-top: 14rpx;
  margin-right: 17rpx;
}
.execute-order-container .address-container .dashed-line {
  height: 30rpx;
  border-left: #ccc 2rpx dashed;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  margin-left: 10rpx;
}
.execute-order-container .address-container .to {
  display: flex;
}
.execute-order-container .address-container .to::before {
  display: block;
  content: '';
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background-color: #ff9d5b;
  margin-top: 14rpx;
  margin-right: 17rpx;
}
.execute-order-container .order-container {
  padding: 25rpx 50rpx;
  margin-top: 30rpx;
  border-top: solid 25rpx #e5e5e5;
}
.execute-order-container .order-container view {
  line-height: 60rpx;
  height: 60rpx;
  color: #555;
  font-size: 28rpx;
  display: flex;
}
.execute-order-container .operate-container {
  padding: 30rpx 40rpx 40rpx 40rpx;
  display: flex;
  justify-content: space-around;
}
.execute-order-container .operate-container .item {
  background-color: #f7f7f7;
  padding: 15rpx 30rpx;
  border-radius: 15rpx;
}
.execute-order-container .operate-container .item .cancel-icon {
  width: 60rpx;
  display: block;
  margin-top: 10px;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .operate-container .item .cancel-text {
  display: block;
  text-align: center;
  color: #fea802;
  margin-top: 15rpx;
  font-size: 28rpx;
}
.execute-order-container .operate-container .item .arrive-start-icon {
  width: 100rpx;
  display: block;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .operate-container .item .arrive-start-text {
  display: block;
  text-align: center;
  color: #34ba97;
  margin-top: -8rpx;
  font-size: 28rpx;
}
.execute-order-container .operate-container .item .drive-start-icon {
  width: 70rpx;
  display: block;
  margin-top: 15rpx;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .operate-container .item .drive-start-text {
  display: block;
  text-align: center;
  color: #1aa4ec;
  margin-top: 8rpx;
  font-size: 28rpx;
}
.execute-order-container .operate-container .item .drive-stop-icon {
  width: 70rpx;
  display: block;
  margin-top: 15rpx;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .operate-container .item .drive-stop-text {
  display: block;
  text-align: center;
  color: #8B61FD;
  margin-top: 8rpx;
  font-size: 28rpx;
}
.execute-order-container .operate-container .item .alarm-icon {
  width: 60rpx;
  display: block;
  margin-top: 8px;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .operate-container .item .alarm-text {
  display: block;
  text-align: center;
  color: #ff5b5b;
  margin-top: 18rpx;
  font-size: 28rpx;
}
.execute-order-container .other-container {
  padding: 30rpx 70rpx;
  display: flex;
  justify-content: space-between;
  border-top: solid 25rpx #e5e5e5;
}
.execute-order-container .other-container .item .location-icon {
  width: 65rpx;
  display: block;
  margin-top: 10px;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .other-container .item .location-text {
  display: block;
  text-align: center;
  color: #777;
  margin-top: 15rpx;
  font-size: 28rpx;
}
.execute-order-container .other-container .item .display-icon {
  width: 65rpx;
  display: block;
  margin-top: 10px;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .other-container .item .display-text {
  display: block;
  text-align: center;
  color: #777;
  margin-top: 15rpx;
  font-size: 28rpx;
}
.execute-order-container .other-container .item .order-icon {
  width: 60rpx;
  display: block;
  margin-top: 14px;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .other-container .item .order-text {
  display: block;
  text-align: center;
  color: #777;
  margin-top: 12rpx;
  font-size: 28rpx;
}
.execute-order-container .other-container .item .service-icon {
  width: 65rpx;
  display: block;
  margin-top: 10px;
  margin-left: auto;
  margin-right: auto;
}
.execute-order-container .other-container .item .service-text {
  display: block;
  text-align: center;
  color: #777;
  margin-top: 15rpx;
  font-size: 28rpx;
}
.bottom-container {
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  margin-top: 10px;
}
.bottom-container .btn {
  background-color: #f0f0f0;
  color: #777;
  font-size: 30rpx;
  padding: 0rpx 40rpx;
  line-height: 48px;
  border-radius: 10rpx;
  border: solid 1rpx #e0e0e0;
  display: flex;
}
.bottom-container .btn .btn-icon-1 {
  width: 38rpx;
  margin-top: 24rpx;
  margin-right: 10rpx;
}
.bottom-container .btn .btn-icon-2 {
  width: 36rpx;
  margin-top: 30rpx;
  margin-right: 10rpx;
}
.bottom-container .btn:active {
  background-color: #dedede;
}
.bottom-container .primary {
  background-color: #2074fd;
  color: #fff;
  padding: 0 65rpx;
}
.bottom-container .primary::after {
