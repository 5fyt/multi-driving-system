page {
  height: 100%;
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding-bottom: 70rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-top: 40rpx;
}
.card {
  background-image: url(data:image/png;base64,);
  background-size: cover;
  border-radius: 15rpx;
  background-repeat: no-repeat;
  color: #fff;
  padding: 30rpx 0rpx;
}
.card .total {
  padding: 0rpx 40rpx;
  font-size: 32rpx;
  justify-content: space-between;
  display: flex;
}
.card .total .recharge {
  display: flex;
}
.card .total .recharge .icon {
  width: 36rpx;
  margin-right: 5rpx;
  margin-top: 7rpx;
}
.card .balance {
  font-size: 60rpx;
  padding: 20rpx 30rpx;
}
.card .balance text {
  font-size: 70rpx;
  font-weight: bold;
}
.card .item-list {
  padding: 0 40rpx;
  display: flex;
  justify-content: space-between;
}
.card .item-list .item {
  text-align: center;
}
.card .item-list .item .title {
  font-size: 28rpx;
  margin-bottom: 10rpx;
  display: flex;
}
.card .item-list .item .title .icon {
  width: 36rpx;
  height: 36rpx;
  margin-right: 5rpx;
  margin-top: 3rpx;
}
.card .item-list .item .amount {
  font-size: 28rpx;
}
.record-container {
  margin-top: 50rpx;
}
.record-container .title {
  border-left: solid 10rpx #2074fd;
  line-height: 1.2;
  padding-left: 10rpx;
  margin-bottom: 30rpx;
  font-size: 32rpx;
  color: #444;
  font-weight: bold;
}
.record-container .summary {
  display: flex;
  justify-content: space-between;
}
.record-container .summary .month {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  color: #444;
  width: 260rpx;
  line-height: 2;
  padding: 0 20rpx;
  border: solid 1rpx #c8c9cc;
  border-radius: 7rpx;
}
.record-container .summary .month .icon {
  width: 20rpx;
  height: auto;
  margin-top: 19rpx;
}
.record-container .summary .total {
  font-size: 30rpx;
  color: #444;
}
.record-container .record-list {
  margin-top: 30rpx;
  border-top: solid 1rpx #e0e0e0;
  border-bottom: solid 1rpx #e0e0e0;
}
.record-container .record-list .record {
  border-bottom: solid 1rpx #e0e0e0;
  padding: 20rpx 10rpx;
  display: flex;
  justify-content: space-between;
}
.record-container .record-list .record:last-child {
  border-bottom: none;
}
.record-container .record-list .record .left {
  display: flex;
}
.record-container .record-list .record .left .date-time {
  margin-right: 40rpx;
}
.record-container .record-list .record .left .date-time .date {
  display: flex;
  margin-bottom: 12rpx;
}
.record-container .record-list .record .left .date-time .date .date-icon {
  width: 34rpx;
  margin-top: 5rpx;
  margin-right: 12rpx;
}
.record-container .record-list .record .left .date-time .date text {
  font-size: 28rpx;
  color: #444;
}
.record-container .record-list .record .left .date-time .time {
  display: flex;
}
.record-container .record-list .record .left .date-time .time .time-icon {
  width: 32rpx;
  margin-top: 5rpx;
  margin-right: 14rpx;
}
.record-container .record-list .record .left .date-time .time text {
  font-size: 28rpx;
  color: #b0b0b0;
}
.record-container .record-list .record .left .content .type {
  display: block;
  font-size: 28rpx;
  color: #444;
  margin-bottom: 12rpx;
}
.record-container .record-list .record .left .content .desc {
  display: block;
  font-size: 28rpx;
  color: #b0b0b0;
}
.record-container .record-list .record .right {
  margin-top: 20rpx;
}
.record-container .record-list .record .right .amount {
  font-size: 38rpx;
  margin-right: 5rpx;
}
.record-container .record-list .record .right .red {
  font-size: 38rpx;
  margin-right: 5rpx;
  color: #ff5555;
}
.record-container .record-list .record .right .green {
  font-size: 38rpx;
  margin-right: 5rpx;
  color: #46b68f;
}
.record-container .record-list .record .right .unit {
  color: #444;
  font-size: 30rpx;
}
.record-container .record-list .empty {
  padding: 40rpx 0;
  text-align: center;
  color: #e0e0e0;
  font-size: 28rpx;
}
.record-container .record-list .empty .no-data {
  width: 150rpx;
  height: 150rpx;
}

