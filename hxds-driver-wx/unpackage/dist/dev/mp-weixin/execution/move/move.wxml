<view><map class="map" style="{{(mapStyle)}}" id="map" scale="15" longitude="{{longitude}}" latitude="{{latitude}}" enable-poi="{{true}}" polyline="{{polyline}}" markers="{{markers}}" data-event-opts="{{[['longpress',[['showHandle',['$event']]]]]}}" bindlongpress="__e"><cover-view hidden="{{!(infoStatus)}}" class="information-container"><cover-image class="close-icon" src="../static/move/close-icon.png" data-event-opts="{{[['tap',[['hideHandle',['$event']]]]]}}" bindtap="__e"></cover-image><cover-view class="info"><cover-view class="label">剩余</cover-view><cover-view class="value">{{distance+"公里"}}</cover-view><cover-view class="label">，预计</cover-view><cover-view class="value">{{duration+"分钟"}}</cover-view></cover-view></cover-view></map></view>