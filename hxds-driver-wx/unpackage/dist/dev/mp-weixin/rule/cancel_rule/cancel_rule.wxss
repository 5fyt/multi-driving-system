page {
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding: 50rpx 40rpx 70rpx 40rpx;
}
.item {
  display: flex;
  font-size: 32rpx;
  color: #444;
  line-height: 1;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}
.item::before {
  content: '';
  display: block;
  width: 10rpx;
  height: 32rpx;
  background-color: #777;
  margin-right: 15rpx;
  border-radius: 3rpx;
}
.desc {
  font-size: 28rpx;
  color: #a0a0a0;
  line-height: 1.7;
  padding-left: 25rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  display: flex;
}
.desc .num {
  margin-right: 10rpx;
}

