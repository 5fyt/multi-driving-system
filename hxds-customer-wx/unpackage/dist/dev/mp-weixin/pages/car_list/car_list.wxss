page {
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding: 10rpx 30rpx 0 30rpx;
}
.page .none {
  display: block;
  width: 50%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 80rpx;
  margin-bottom: 40rpx;
}
.page .btn {
  background-color: #2074fd;
  color: #fff;
  width: 100%;
  margin-top: 50rpx;
  font-size: 30rpx;
  line-height: 3;
}
.page .btn::after {
  border: none;
}
.page .btn:active {
  background-color: #0259e9;
}
.page .row {
  border-bottom: solid 1rpx #e0e0e0;
  padding: 30rpx 20rpx;
  display: flex;
  justify-content: space-between;
}
.page .row .icon {
  vertical-align: -4rpx;
  margin-right: 10rpx;
}

