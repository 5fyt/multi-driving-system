<view><map class="map" style="{{(contentStyle)}}" id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="15" enable-traffic="{{false}}" show-location="{{true}}" enable-poi="{{true}}"><cover-image class="location" src="../../static/workbench/location.png" data-event-opts="{{[['tap',[['returnLocationHandle']]]]}}" bindtap="__e"></cover-image></map><view class="panel"><view data-event-opts="{{[['tap',[['chooseLocationHandle',['from']]]]]}}" class="from" bindtap="__e"><text>{{from.address}}</text></view><view class="dashed-line"></view><view data-event-opts="{{[['tap',[['chooseLocationHandle',['to']]]]]}}" class="to" bindtap="__e"><text>{{to.address}}</text></view></view></view>